source "https://cdn.cocoapods.org"
use_frameworks!
use_modular_headers!

platform :ios, :deployment_target => '14.2'
install! 'cocoapods', :deterministic_uuids => false

def default_pods
  pod 'Firebase'
  pod 'Firebase/Analytics'
  pod 'Firebase/Crashlytics'
  pod 'FirebaseCore'
  pod 'FirebaseMessaging'
  pod 'FirebaseCoreDiagnostics'
  pod 'GoogleUtilities'
  pod 'Kingfisher'
  pod 'shared', :path => '../shared'
end

target 'papakarlo' do
  default_pods
end

target 'yuliar' do
  default_pods
end

target 'djan' do
  default_pods
end

target 'gustopub' do
  default_pods
end