CREATE TABLE streetEntity (
    uuid TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    cityUuid TEXT NOT NULL
);

insertStreet:
INSERT OR REPLACE
INTO streetEntity
VALUES (?, ?, ?);

getStreetListByCityUuid:
SELECT *
FROM streetEntity
WHERE cityUuid = :cityUuid;

getStreetByNameAndCityUuid:
SELECT *
FROM streetEntity
WHERE name = :name AND cityUuid = :cityUuid;